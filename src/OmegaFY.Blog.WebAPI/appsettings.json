{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Database": "[SECRET]",
    "SqlLite": "[SECRET]",
    "SqlServer": "[SECRET]",
    "MongoDB": "[SECRET]",
    "CosmoDB": "[SECRET]",
    "RavenDB": "[SECRET]"
  },
  "JwtSettings": {
    "Audience": "[SECRET]",
    "Issuer": "[SECRET]",
    "Secret": "[SECRET]",
    "TimeToExpireToken": "[SECRET]",
    "TimeToExpireRefreshToken": "[SECRET]"
  },
  "AuthenticationSettings": {
    "PasswordRequireDigit": true,
    "PasswordRequireLowercase": true,
    "PasswordRequireNonAlphanumeric": true,
    "PasswordRequireUppercase": true,
    "PasswordMinRequiredLength": 10,
    "PasswordMaxRequiredLength": 64,
    "PasswordRequiredUniqueChars": 5,
    "DefaultLockoutTimeSpan": "00:03:00",
    "MaxFailedAccessAttempts": 3,
    "RequireUniqueEmail": true,
    "RequireConfirmedEmail": false,
    "RequireConfirmedAccount": false
  },
  "AllowedHosts": "*",
  "OpenTelemetrySettings": {
    "ServiceName": "[SECRET]",
    "HoneycombApiKey": "[SECRET]",
    "HoneycombUrl": "[SECRET]"
  },
  "SendGridSettings": {
    "ApiKey": "[SECRET]",
    "FromEmail": "[SECRET]",
    "FromEmailDisplayName": "[SECRET]"
  },
  "IpOrUserTokenBucketPolicySettings": {
    "ReplenishmentPeriod": "00:01:00",
    "TokenLimit": "100",
    "TokensPerPeriod": "100"
  }
}